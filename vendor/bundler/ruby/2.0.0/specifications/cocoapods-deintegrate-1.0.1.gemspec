# -*- encoding: utf-8 -*-

Gem::Specification.new do |s|
  s.name = "cocoapods-deintegrate"
  s.version = "1.0.1"

  s.required_rubygems_version = Gem::Requirement.new(">= 0") if s.respond_to? :required_rubygems_version=
  s.authors = ["Kyle Fuller"]
  s.date = "2016-08-30"
  s.email = ["kyle@fuller.li"]
  s.homepage = "https://github.com/kylef/cocoapods-deintegrate"
  s.licenses = ["MIT"]
  s.require_paths = ["lib"]
  s.required_ruby_version = Gem::Requirement.new(">= 2.0.0")
  s.rubygems_version = "2.0.14.1"
  s.summary = "A CocoaPods plugin to remove and de-integrate CocoaPods from your project."
end
